@using System.Runtime.InteropServices.JavaScript
<div class="card border-secondary mb-3" style="max-width: 18rem;">
    <div class="card-header">@Federal_State</div>
<div class="card-body text-secondary">
    <h5 class="card-title">@Name</h5>
    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
</div>
</div>

@code {
     [Parameter] public string Name { get; set; }
     [Parameter] public int Members { get; set; } 
     [Parameter] public DateTime From_Date { get; set; }
     [Parameter] public DateTime To_Date { get; set; }
     [Parameter] public string Description { get; set; }
     [Parameter] public string Federal_State { get; set; }
     
}